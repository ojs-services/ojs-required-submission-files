msgid "plugins.generic.requiredFiles.displayName"
msgstr "Required Submission Files"

msgid "plugins.generic.requiredFiles.description"
msgstr "Allows editors to mark specific article component types as required during submission. Authors must upload at least one file of each required type before proceeding past the upload step."

msgid "plugins.generic.requiredFiles.settings.description"
msgstr "Select which file types authors must upload during submission"

msgid "plugins.generic.requiredFiles.settings.note"
msgstr "This list reflects the article components defined in your journal. To add or modify component types, go to Workflow > Submission > Components."

msgid "plugins.generic.requiredFiles.validation.missingFiles"
msgstr "The following required files are missing: {$fileTypes}. Please upload at least one file of each required type before continuing."

msgid "plugins.generic.requiredFiles.validation.errorTitle"
msgstr "Missing Required Files"

msgid "plugins.generic.requiredFiles.validation.uploadError"
msgstr "Upload failed. Please try again."

msgid "plugins.generic.requiredFiles.banner.title"
msgstr "Required Files:"

msgid "plugins.generic.requiredFiles.section.required"
msgstr "Required Files"

msgid "plugins.generic.requiredFiles.section.optional"
msgstr "Uploaded Files"

msgid "plugins.generic.requiredFiles.status.notUploaded"
msgstr "Not uploaded"

msgid "plugins.generic.requiredFiles.status.uploaded"
msgstr "Uploaded"

